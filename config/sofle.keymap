    keymap {
        compatible = "zmk,keymap";

        default_layer {
            display-name = "Base";

            // ------------------------------------------------------------------------------------------------------------
            // |   `   |  1  |  2  |  3   |  4   |  5   |                   |  6   |  7    |  8    |  9   |   0   |       |
            // |  ESC  |  Q  |  W  |  E   |  R   |  T   |                   |  Y   |  U    |  I    |  O   |   P   | BKSPC |
            // |  TAB  |  A  |  S  |  D   |  F   |  G   |                   |  H   |  J    |  K    |  L   |   ;   |   '   |
            // | SHIFT |  Z  |  X  |  C   |  V   |  B   |  MUTE  |  |       |  N   |  M    |  ,    |  .   |   /   | SHIFT |
            //               | GUI | ALT  | CTRL | LOWER|  ENTER |  | SPACE | RAISE| CTRL  | ALT   | GUI  |

            bindings = <
&gresc        &kp N1  &kp N2    &kp N3    &kp N4     &kp N5                                       &kp N6  &kp N7  &kp N8     &kp N9   &kp N0    &kp BACKSPACE
&kp GRAVE     &kp Q   &kp W     &kp E     &kp R      &kp T                                        &kp Y   &kp U   &kp I      &kp O    &kp P     &kp LEFT_BRACKET
&kp TAB       &kp A   &kp S     &kp D     &kp F      &kp G                                        &kp H   &kp J   &kp K      &kp L    &kp SEMI  &kp SQT
&kp CAPSLOCK  &kp Z   &kp X     &kp C     &kp V      &kp B           &kp K_MUTE    &kp RU_NUMERO  &kp N   &kp M   &kp COMMA  &kp DOT  &kp FSLH  &kp RIGHT_BRACKET
                      &kp LGUI  &kp LALT  &kp LCTRL  &kp LEFT_SHIFT  &kp SPACE     &kp ENTER      &mo 1   &mo 2   &kp PIPE   &mo 3
            >;

            sensor-bindings =
                <&inc_dec_kp RIGHT_ARROW LEFT_ARROW>,
                <&inc_dec_kp DOWN_ARROW UP_ARROW>;
        };

        lower_layer {
            display-name = "Symbols";

            // TODO: Some binds are waiting for shifted keycode support.
            // ------------------------------------------------------------------------------------------------------------
            // |       |  F1 |  F2 |  F3  |  F4  |  F5  |                   |  F6  |  F7   |  F8   |  F9  |  F10  |  F11  |
            // |   `   |  1  |  2  |  3   |  4   |  5   |                   |  6   |  7    |  8    |  9   |   0   |  F12  |
            // |       |  !  |  @  |  #   |  $   |  %   |                   |  ^   |  &    |  *    |  (   |   )   |   |   |
            // |       |  =  |  -  |  +   |  {   |  }   |        |  |       |  [   |  ]    |  ;    |  :   |   \   |       |
            //               |     |      |      |      |        |  |       |      |       |       |      |

            bindings = <
&trans  &kp F1  &kp F2  &kp F3      &kp F4  &kp F5                    &kp F6      &kp F7     &kp F8           &kp F9                &kp F10                &kp F11
&trans  &se_a   &se_o   &trans      &trans  &trans                    &kp INS     &kp HOME   &kp PRINTSCREEN  &kp LEFT_PARENTHESIS  &kp RIGHT_PARENTHESIS  &kp F12
&trans  &de_a   &de_o   &de_esszet  &de_u   &trans                    &kp DELETE  &kp END    &kp SCROLLLOCK   &kp MINUS             &kp PLUS               &emdash
&trans  &trans  &trans  &trans      &trans  &trans  &trans    &trans  &trans      &kp K_CUT  &kp K_COPY       &kp K_PASTE           &trans                 &trans
                &trans  &trans      &trans  &trans  &trans    &trans  &trans      &trans     &trans           &trans
            >;

            sensor-bindings =
                <&inc_dec_kp K_UNDO K_REDO>,
                <&inc_dec_kp PAGE_UP PAGE_DOWN>;
        };

        raise_layer {
            display-name = "stinkybutts";

            // ------------------------------------------------------------------------------------------------------------
            // | BTCLR | BT1  | BT2  |  BT3  |  BT4  |  BT5 |                |      |      |       |      |       |       |
            // |       | INS  | PSCR | GUI   |       |      |                | PGUP |      |   ^   |      |       |       |
            // |       | ALT  | CTRL | SHIFT |       | CAPS |                | PGDN |   <- |   v   |  ->  |  DEL  | BKSPC |
            // |       | UNDO | CUT  | COPY  | PASTE |      |      |  |      |      |      |       |      |       |       |
            //                |      |       |       |      |      |  |      |      |      |       |      |

