// Encoders

&sensors { triggers-per-rotation = <30>; };

&left_encoder { steps = <90>; };

&right_encoder { steps = <90>; };

#define BASE 0
#define LOWER 1
#define RAISE 2
#define ADJUST 3

/ {
    // Activate ADJUST layer by pressing raise and lower

    conditional_layers {
        compatible = "zmk,conditional-layers";

        adjust_layer {
            if-layers = <1 2>;
            then-layer = <4>;
        };
    };

    combos {
        compatible = "zmk,combos";

        esc {
            bindings = <&kp ESCAPE>;
            key-positions = <15 14>;
        };
    };

    behaviors {
        caps: caps {
            compatible = "zmk,behavior-caps-word";
            label = "CAPS";
            #binding-cells = <0>;
            continue-list = <POUND MINUS BACKSPACE UNDERSCORE>;
        };

        sp_sh: space_shift {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            tapping-term-ms = <100>;
            flavor = "balanced";
            bindings = <&kp>, <&kp>;
        };

        td_spc: tap_dance_space {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <100>;
            bindings = <&sp_sh LSHFT SPACE>, <&kp CAPS>;
        };

        // Hold-tap for delete: hold = repeat delete, tap = pass through

        ht_del: hold_tap_delete {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            tapping-term-ms = <200>;
            flavor = "tap-preferred";
            bindings = <&kp>, <&kp>;
        };

        // Tap-dance for delete: tap = DEL, double-tap = Ctrl+DEL

        td_del: tap_dance_delete {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <170>;
            bindings = <&ht_del DEL DEL>, <&kp LC(DEL)>;
        };

        // Tap-dance for backspace: tap = BSPC, double-tap = Ctrl+BSPC

        td_bspc: tap_dance_backspace {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <150>;
            bindings = <&ht_del BSPC BSPC>, <&kp LC(BSPC)>;
        };
    };

